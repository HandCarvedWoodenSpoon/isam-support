{
	"name":"exampleIDP",
	"protocol":"SAML2_0",
	"role":"ip",
	"templateName":"",
	"configuration":{
		"accessPolicy":"",
		"artifactLifetime":300,
		"assertionSettings":{
			"assertionValidBefore":60,
			"assertionValidAfter":60
		},
		"artifactResolutionService":[
			{
				"binding":"soap",
				"default":false,
				"index":0,
				"url":"https://identity.hyperv.lab/isam/sps/exampleIDP/saml20/soap"
			}
		],
		"attributeMapping":{

		},
		"companyName":"companyNamePlaceholder",
		"encryptionSettings":{
			"decryptionKeyIdentifier":null
		},
		"identityMapping":{
			"activeDelegateId":"default-map",
			"properties":{
				"ruleType":"JAVASCRIPT",
				"identityMappingRuleReference":"110"
			}
		},
		"extensionMapping":{
			"activeDelegateId":"skip-extension-map",
			"properties":{
				
			}
		},
		"manageNameIDService":[
			{
				"binding":"artifact",
				"url":"https://identity.hyperv.lab/isam/sps/exampleIDP/saml20/mnids"
			},
			{
				"binding":"post",
				"url":"https://identity.hyperv.lab/isam/sps/exampleIDP/saml20/mnids"
			},
			{
				"binding":"redirect",
				"url":"https://identity.hyperv.lab/isam/sps/exampleIDP/saml20/mnids"
			},
			{
				"binding":"soap",
				"url":"https://identity.hyperv.lab/isam/sps/exampleIDP/saml20/soap"
			}
		],
		"messageValidTime":300,
		"messageIssuerFormat":"urn:oasis:names:tc:SAML:2.0:nameid-format:entity",
		"messageIssuerNameQualifier":"",
		"nameIDFormat":{
			"default":"urn:oasis:names:tc:SAML:1.1:nameid-format:emailAddress",
			"supported":[
				"urn:oasis:names:tc:SAML:2.0:nameid-format:persistent",
				"urn:oasis:names:tc:SAML:1.1:nameid-format:emailAddress",
				"urn:oasis:names:tc:SAML:2.0:nameid-format:transient",
				"urn:oasis:names:tc:SAML:1.1:nameid-format:unspecified"
			]
		},
		"needConsentToFederate":true,
		"excludeSessionIndexInSingleLogoutRequest":false,
		"pointOfContactUrl":"https://identity.hyperv.lab/isam/sps",
		"providerId":"https://identity.hyperv.lab/isam/sps/exampleIDP/saml20",
		"sessionTimeout":7200,
		"signatureSettings":{
			"keyInfoElements":{
				"includePublicKey":false,
				"includeX509CertificateData":true,
				"includeX509IssuerDetails":false,
				"includeX509SubjectKeyIdentifier":false,
				"includeX509SubjectName":false
			},
			"signingKeyIdentifier":{
				"keystore":"rt_profile_keys",
				"label":"server"
			},
			"signingOptions":{
				"signArtifactRequest":false,
				"signArtifactResponse":false,
				"signAssertion":false,
				"signAuthnResponse":false,
				"signLogoutRequest":false,
				"signLogoutResponse":false,
				"signNameIDManagementRequest":false,
				"signNameIDManagementResponse":false
			},
			"transformAlgorithmElements":{
				"includeInclusiveNamespaces":true
			},
			"validatingOptions":{
				"validateAssertion":false,
				"validateAuthnRequest":false
			}
		},
		"singleSignOnService":[
			{
				"binding":"artifact",
				"url":"https://identity.hyperv.lab/isam/sps/exampleIDP/saml20/login"
			},
			{
				"binding":"post",
				"url":"https://identity.hyperv.lab/isam/sps/exampleIDP/saml20/login"
			},
			{
				"binding":"redirect",
				"url":"https://identity.hyperv.lab/isam/sps/exampleIDP/saml20/login"
			}
		],
		"singleLogoutService":[
			{
				"binding":"artifact",
				"url":"https://identity.hyperv.lab/isam/sps/exampleIDP/saml20/slo"
			},
			{
				"binding":"post",
				"url":"https://identity.hyperv.lab/isam/sps/exampleIDP/saml20/slo"
			},
			{
				"binding":"redirect",
				"url":"https://identity.hyperv.lab/isam/sps/exampleIDP/saml20/slo"
			},
			{
				"binding":"soap",
				"url":"https://identity.hyperv.lab/isam/sps/exampleIDP/saml20/soap"
			}
		],
		"aliasServiceSettings":{
			"aliasServiceDBType":"jdbc"
		}
	}
}